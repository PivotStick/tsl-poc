.menu-background {
  background-image: url("project://database/Assets/Textures/UI/2MainMenu/background.png");
  -unity-background-scale-mode: scale-and-crop;
  height: 100%;
  width: 100%;
}

.on-panel-text {
  -unity-font: url("project://database/Assets/Textures/UI/fonts/Exo-Medium.ttf");
  -unity-font-definition: url("project://database/Assets/Textures/UI/fonts/Exo-Medium SDF.asset");

  -unity-font-style: bold;

  font-size: 22px;

  color: rgb(0, 162, 255);
}

.unity-button {
  background-image: url("project://database/Assets/Textures/UI/Components5/Btn_button03_n.png");
  background-color: transparent;
  border-width: 0;

  -unity-font-style: bold;

  margin: 0;
  padding: 32px;
}

.unity-button:hover {
  background-image: url("project://database/Assets/Textures/UI/Components5/Btn_button03_h.png");
  background-color: transparent;
}

.unity-button:active {
  background-image: url("project://database/Assets/Textures/UI/Components5/Btn_button03_p.png");
  background-color: transparent;
}
