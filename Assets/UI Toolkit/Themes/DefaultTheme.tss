@import url("UnityDefaultRuntimeTheme.tss");

:root {
  color: white;
}

.large-panel-01 {
  background-image: url("project://database/Assets/Textures/UI/Components6/Large panel01.png");
}

.large-panel-02 {
  background-image: url("project://database/Assets/Textures/UI/Components6/Large panel02.png");
}

.large-panel-03 {
  background-image: url("project://database/Assets/Textures/UI/Components6/Large panel03.png");
}

.large-panel-04 {
  background-image: url("project://database/Assets/Textures/UI/Components6/Large panel04.png");
}

.middle-panel-01 {
  background-image: url("project://database/Assets/Textures/UI/Components6/middle panel01.png");
}

.middle-panel-02 {
  background-image: url("project://database/Assets/Textures/UI/Components6/middle panel02.png");
